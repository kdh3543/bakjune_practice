package bakjune_practice_¹è¿­;

import java.util.Scanner;

public class Num_10_´Ù½ÃÇ®¾îºÁ¾ßµÊ {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int C =	sc.nextInt(); // 5
		int[] N = new int[C]; // 5
		double[] avg = new double[C];
		
		for(int i = 0; i < C; i++) {
			N[i] = sc.nextInt(); // N[0]=5,N[1]=7,N[2]=3,N[3]=3,N[4]=9
			int sum = 0;
			for(int j = 0; j<N[i];j++) {
				N[j] = sc.nextInt();
				sum += N[j];
			}
			avg[i] = (double)sum/N[i];			
		}
		
		System.out.println(avg[0]);
		
		
	}
}
