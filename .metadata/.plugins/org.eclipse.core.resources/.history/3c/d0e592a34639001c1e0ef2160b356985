package bakjune_practice_¹è¿­;

import java.util.Scanner;

public class Num_10 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int C =	sc.nextInt();
		double[] avg = new double[C];
		int N = 0;
		int[] score = null;
		for(int i =0;i<C;i++) {
			N = sc.nextInt();
			int sum = 0;
			score = new int[N];
			for(int j =0;j<score.length;j++) {
				score[j] = sc.nextInt();
				sum += score[j];
				avg[i] = (double)sum/N;
			}
		}
		int[] result = new int[C];
		for(int i = 0; i<C;i++) {
			result[i] = 0;
			for(int j =0; j<score.length;j++) {
				if(avg[i]<score[j] ) {
					result[i]++;
				}
			}
		}
		
		for(int i = 0; i<C;i++) {
			System.out.println(Math.round((double)result[i]/N*1000)/1000.0);
		}


	}
}
