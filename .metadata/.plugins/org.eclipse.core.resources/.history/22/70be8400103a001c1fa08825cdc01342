package bakjune_practice_¹è¿­;

import java.util.Scanner;

public class Num_10_´Ù½ÃÇ®¾îºÁ¾ßµÊ {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int C =	sc.nextInt();
		int[] N = null;
		for(int i=0;i<C;i++) {
			N =new int[C];
			N[i] = sc.nextInt();
			
		}
		int[] score = null;
		double[] avg = null;
		for(int i = 0; i<N.length;i++) {
			score = new int[N[i]];
			score[i] = sc.nextInt();
			avg = new double[N[i]];
		}
		int[] count = new int[C];
		for(int i = 0; i<C;i++) {
			int sum =0;
			count[i] = 0;
			for(int j = 0; j<score.length;j++) {
				sum += score[j];
			}
			avg[i] = (double)sum/N[i];
			for(int m = 0; m<N[i];m++) {
				if(avg[i]<score[N[i]]) {
					count[i]++;
				}
			}
		}
		for(int i = 0; i<C;i++) {
			System.out.printf("%.3f",(double)count[i]/N[i]);
		}
		
		
	}
}
