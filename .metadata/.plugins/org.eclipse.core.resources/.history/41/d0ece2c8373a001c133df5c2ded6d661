package bakjune_practice_¹®ÀÚ¿­;

import java.util.Scanner;

public class Num1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		String N2 = sc.nextLine();
		int sum = 0;
		for(int i = 0; i < N2.length(); i++) {
			sum += N2.charAt(i);
		}
		
		System.out.println(sum);
	}	

}
