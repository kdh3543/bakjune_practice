package bakjune_practice_배열;

import java.util.Scanner;

public class Num_3 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("테스트 케이스의 개수: ");
		int testcase = sc.nextInt();
		int[] test = new int[testcase];
		int[] num = null;
		int[] avg = null;
		int sum = 0;
		for(int i = 0; i<testcase; i++) {
			System.out.print(i+1+"번째 케이스의 학생 수: ");
			test[i] = sc.nextInt();
			num = new int[test[i]];	
			for(int j = 0; j<test[i]; j++) {
				System.out.print(j+1+"번째 학생의 점수: ");
				num[j] = sc.nextInt();
				sum += num[j];
			}
			avg[i] = sum/test[i];
			
		}
		for(int i = 0; i<testcase;i++) {
			System.out.println(avg[i]);
		}
		

	}

}
