package bakjune_practice_for¹®;

import java.util.Scanner;

public class Num10 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);	
		int N = Integer.parseInt(sc.nextLine());
		for(int i = N; i>0;i--) {
			for(int j = i-1;j>0;j--) {
				System.out.print(" ");
			}
			for(int j = N;j>0;j--) {
				System.out.print("*");
			}
			System.out.println();
		}		
	}
}
