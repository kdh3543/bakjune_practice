package bakjune_practice_¹è¿­;

import java.util.Scanner;

public class Num_5 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int[] arr = new int[N];
		
		for(int i = 0; i<arr.length;i++) {
			arr[i] = sc.nextInt();
		}
		
		int max = arr[0];
		int count = 0;
		
		for(int i = 0; i<arr.length;i++) {
			if(max<arr[i]) {
				max = arr[i];
				count++;
			}else {
				continue;
			}
			
			
		}
		System.out.println(count+" "+max);
	}
}
